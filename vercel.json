{
  "framework": "laravel",
  "php": "8.2",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": false,
    "APP_URL": "@APP_URL"
  },
  "build": {
    "web": [
      {
        "src": "public/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php",
        "methods": ["GET", "HEAD", "PUT", "POST", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Cache-Control": "public, max-age=3600"
        }
      },
      {
        "src": "/storage/(.*)",
        "status": 404
      }
    ]
  },
  "functions": {
    "api/index.php": {
      "runtime": "php:8.2",
      "memory": 3008,
      "maxDuration": 60,
      "includeFiles": "**/*.php"
    }
  },
  "installCommand": "npm ci && composer install --optimize-autoloader --no-dev",
  "buildCommand": "npm run build && php artisan config:cache && php artisan route:cache",
  "outputDirectory": "public",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
