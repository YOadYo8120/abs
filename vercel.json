{
  "framework": null,
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php",
      "methods": ["GET", "HEAD", "PUT", "POST", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/storage/(.*)",
      "status": 404
    }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "installCommand": "npm ci && composer install --optimize-autoloader --no-dev",
  "buildCommand": "npm run build && php artisan config:cache && php artisan route:cache",
  "outputDirectory": "public",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
