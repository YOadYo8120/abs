{
  "framework": null,
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2",
      "memory": 512,
      "maxDuration": 30
    }
  }
}
