{
  "framework": null,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2",
      "memory": 512,
      "maxDuration": 30
    }
  },
  "installCommand": "npm ci && composer install --optimize-autoloader --no-dev",
  "buildCommand": "npm run build && php artisan config:cache && php artisan route:cache && php artisan migrate --force"
}
